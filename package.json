{
  "name": "mapo-next",
  "version": "2.0.0",
  "description": "Monorepo for @mapomodule in Nuxt 3",
  "main": "index.js",
  "workspaces": [
    "packages/layers/*",
    "packages/example"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "example:dev": "pnpm --filter mapo-example dev",
    "core:dev": "pnpm --filter mapo-core-layer dev",
    "uikit:dev": "pnpm --filter mapo-uikit-layer dev",
    "uikit:storybook": "pnpm --filter mapo-uikit-layer storybook",
    "dev": "pnpm --stream -r -parallel dev"
  },
  "keywords": [],
  "author": "Michele Abbrescia",
  "license": "ISC",
  "private": true,
  "devDependencies": {
    "sass": "^1.85.1"
  },
  "engines": {
    "node": ">=22.13.1",
    "npm": ">=10.5.2"
  },
  "packageManager": "pnpm@10.5.2+sha512.da9dc28cd3ff40d0592188235ab25d3202add8a207afbedc682220e4a0029ffbff4562102b9e6e46b4e3f9e8bd53e6d05de48544b0c57d4b0179e22c76d1199b"
}
